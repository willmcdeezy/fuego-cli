{
  "name": "@fuego/cli",
  "version": "0.1.0",
  "description": "Command-line interface for Fuego - the sovereign Solana wallet for AI agents",
  "main": "dist/index.js",
  "type": "module",
  "bin": {
    "fuego": "./dist/index.js"
  },
  "scripts": {
    "build": "npx tsc",
    "dev": "npx tsc --watch",
    "start": "node dist/index.js",
    "test": "node --test dist/**/*.test.js",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write src/**/*.ts"
  },
  "keywords": [
    "solana",
    "wallet",
    "cli",
    "ai-agent",
    "cryptocurrency",
    "blockchain",
    "fuego"
  ],
  "author": "Will McDeezy <will@yatori.io>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/willmcdeezy/fuego-cli.git"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@solana/web3.js": "^1.98.0",
    "commander": "^12.0.0",
    "chalk": "^5.3.0",
    "ora": "^8.0.1",
    "inquirer": "^12.0.0",
    "fs-extra": "^11.2.0",
    "conf": "^13.0.0",
    "node-fetch": "^3.3.2"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "@types/fs-extra": "^11.0.4",
    "typescript": "^5.3.3",
    "eslint": "^8.56.0",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "prettier": "^3.2.4"
  }
}
